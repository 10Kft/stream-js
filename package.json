{
  "author": {
    "name": "Thierry Schellenbach",
    "company": "Stream.io Inc"
  },
  "name": "getstream",
  "description": "The official low-level GetStream.io client for Node.js and the browser.",
  "main": "./src/getstream.js",
  "homepage": "https://getstream.io/",
  "email": "support@getstream.io",
  "version": "3.5.0",
  "scripts": {
    "test": "npm run test-unit-node",
    "test-unit-node": "./node_modules/.bin/mocha test/unit/common test/unit/node",
    "test-unit-browser": "./node_modules/.bin/karma start karma.config.js --single-run",
    "test-integration-node": "./node_modules/.bin/mocha test/integration/common test/integration/node",
    "test-integration-browser": "./node_modules/.bin/mocha test/integration/common test/integration/browser",
    "generate-docs": "jsdoc -c .jsdoc",
    "coverage": "./node_modules/.bin/istanbul bin/run-node-unit-tests.js",
    "preversion": "npm test",
    "version": "gulp build && git add dist && git checkout test",
    "postversion": "./bin/postversion.sh"
  },
  "browser": {
    "request": "xmlhttp-request",
    "crypto": false,
    "jsonwebtoken": false,
    "./src/lib/batch_operations.js": false,
    "qs": false,
    "url": false
  },
  "react-native": {
    "request": "xmlhttp-request",
    "crypto": false,
    "jsonwebtoken": false,
    "./src/lib/batch_operations.js": false,
    "qs": false,
    "url": false
  },
  "config": {
    "blanket": {
      "pattern": "src",
      "data-cover-never": [
        "node_modules"
      ]
    }
  },
  "devDependencies": {
    "async": ">=0.9.0",
    "babel-core": ">=5.8.25",
    "babel-loader": ">=5.3.2",
    "bluebird": ">=2.1.3",
    "codeclimate-test-reporter": ">=0.3.1",
    "connect": ">=3.0.1",
    "coveralls": ">=2.10.1",
    "eslint": ">=1.10.3",
    "expect.js": ">=0.3.1",
    "gulp": ">=3.8.7",
    "gulp-browserify": ">=0.5.0",
    "gulp-jscs": ">=3.0.1",
    "gulp-jscs-stylish": ">=1.2.1",
    "gulp-jshint": ">=1.6.3",
    "gulp-mocha": ">=0.4.1",
    "gulp-uglify": ">=0.3.1",
    "gulp-util": ">=2.2.17",
    "imports-loader": "~0.6.5",
    "inject-loader": "~3.0.0-beta2",
    "istanbul": "~0.4.5",
    "jshint": "^2.9.3",
    "jshint-stylish": ">=2.0.1",
    "karma": "~1.3.0",
    "karma-chrome-launcher": "~2.0.0",
    "karma-mocha": "~1.2.0",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "~1.8.0",
    "mocha": "^3.1.0",
    "mocha-lcov-reporter": ">=0.0.1",
    "mocha-sauce": "git://github.com/pbakaus/mocha-sauce.git",
    "nock": ">=5.2.1",
    "quickcheck": ">=0.0.4",
    "rewire": "~2.5.2",
    "rewire-webpack": "~1.0.1",
    "serve-static": ">=1.2.3",
    "term-canvas": ">=0.0.5",
    "testdouble": "~1.7.0",
    "webpack": ">=1.12.2"
  },
  "license": "BSD-3-Clause",
  "dependencies": {
    "Base64": ">=0.3.0",
    "xmlhttp-request": ">=0.4.0",
    "faye": ">=1.2.0",
    "http-signature": ">=1.0.2",
    "jsonwebtoken": "5.0.1",
    "qs": ">=6.2.0",
    "request": ">=2.67.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/GetStream/stream-js.git"
  },
  "engines": {
    "node": ">=0.11 <=5.7"
  }
}
